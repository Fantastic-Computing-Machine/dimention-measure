{
    "version": 2,
    "builds": [
      { "src": "manage.py", "use": "@vercel/python" },
      { "src": "requirements.txt", "use": "@vercel/python" },
      { "src": "static/**", "use": "@vercel/static" },
      { "src": "templates/**", "use": "@vercel/python" }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/manage.py"
      }
    ]
  }
  